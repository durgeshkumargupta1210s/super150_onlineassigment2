import java.util.*;
public class Main {
    public static void main(String args[]) {
    Scanner sc = new Scanner(System.in);
    int num1 = sc.nextInt();
    int num2 = sc.nextInt();
    // for(int i=1;i<Integer.MAX_VALUE; i++){
    for(int i=1;i<(num1*num2); i++){
        if(i%num1==0 && i%num2==0){
            System.out.println(i);
            break;
        }
    }


    }
}
