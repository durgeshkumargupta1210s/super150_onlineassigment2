import java.util.*;
public class Main {
    public static void main(String args[]) {
    Scanner sc = new Scanner(System.in);
    int num = sc.nextInt();
    int dec=0;
    int mul=0;
    while(num>0){
        int rem=num%10;
        dec=dec+(int)(rem*(Math.pow(2,mul)));
        num=num/10;
        mul++;

    }
    System.out.println(dec);

    }
}
